# DO NOT EDIT! This file will be overwritten by LXAppearance.
# Any customization should be done in ~/.gtkrc-2.0.mine instead.

gtk-theme-name="Zukitwo"
gtk-icon-theme-name="Faenza"
gtk-font-name="Droid Sans 10"
gtk-cursor-theme-size=0
gtk-toolbar-style=GTK_TOOLBAR_ICONS
gtk-toolbar-icon-size=GTK_ICON_SIZE_SMALL_TOOLBAR
gtk-button-images=1
gtk-menu-images=1
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintfull"
gtk-xft-rgba="rgb"
gtk-color-scheme="tooltip_fg_color:#FFFFFF\nbase_color:#ffffff\nselected_fg_color:#ffffff\ntext_color:#313739\nbg_color:#EDEDED\ntooltip_bg_color:#343434\nselected_bg_color:#4A90D9\nfg_color:#000000\n"
#include "/home/blade/.gtkrc-2.0.mine"
